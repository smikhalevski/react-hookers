{
  "name": "React Hookers",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "disableSources": true,
  "hideGenerator": true,
  "readme": "none",
  "tsconfig": "./tsconfig.json",
  "entryPoints": ["./src/main/index.ts"],
  "navigation": {
    "includeCategories": true,
    "includeFolders": false,
    "includeGroups": true
  },
  "compilerOptions": {
    "skipLibCheck": true
  },
  "plugin": ["typedoc-plugin-mdn-links"],
  "kindSortOrder": ["Function", "Class", "Variable", "TypeAlias"],
  "intentionallyNotExported": ["UnionToIntersection"]
}
